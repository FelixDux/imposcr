    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app"
            ],
            "jinja": true
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'imposcr'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=imposcr",
                    "--package=imposcr"
                ],
                "filter": {
                    "name": "imposcr",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'imposcr'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=imposcr",
                    "--package=imposcr"
                ],
                "filter": {
                    "name": "imposcr",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}